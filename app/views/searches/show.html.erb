<div class="container">
  <div id="search-box">
  <%= form_tag("/search", method: "get", class: "form-inline") do %>
    <%= label_tag(:keywords, "") %>
    <%= text_field_tag(:keywords, @keywords, class: "form-control") %>
    <%= submit_tag("Search", class: "btn btn-primary") %>
  <% end %>
  </div>
</div>

<%= render "search_results"%>
